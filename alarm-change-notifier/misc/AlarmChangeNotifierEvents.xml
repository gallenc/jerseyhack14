<?xml version="1.0" encoding="UTF-8"?>
<events xmlns="http://xmlns.opennms.org/xsd/eventconf">
  <event>
    <uei>uei.opennms.org/plugin/AlarmChangeNotificationEvent/AlarmDeleted</uei>
    <event-label>Alarm %parm[alarmid]% Deleted</event-label>
    <descr>
    &lt;p&gt;Alarm %parm[alarmid]% Deleted&lt;p&gt;
    
    &lt;p&gt;Deleted Alarm Values&lt;p&gt;
    &lt;p&gt;%parm[oldalarmvalues]%&lt;p&gt;
    </descr>
    <logmsg dest="logndisplay">
    &lt;p&gt;Alarm %parm[alarmid]% Deleted&lt;p&gt;
    </logmsg>
    <severity>Normal</severity>
    <operinstruct></operinstruct>
  </event>

  <event>
    <uei>uei.opennms.org/plugin/AlarmChangeNotificationEvent/NewAlarmCreated</uei>
    <event-label>New Alarm Created %parm[alarmid]%</event-label>
    <descr>
    &lt;p&gt;New Alarm %parm[alarmid]% Created&lt;p&gt;
    
    &lt;p&gt;New Alarm Values&lt;p&gt;
    &lt;p&gt;%parm[newalarmvalues]%&lt;p&gt;
    </descr>
    <logmsg dest="logndisplay">
    &lt;p&gt;New Alarm %parm[alarmid]% Created&lt;p&gt;
    </logmsg>
    <severity>Normal</severity>
    <operinstruct></operinstruct>
  </event>

  <event>
    <uei>uei.opennms.org/plugin/AlarmChangeNotificationEvent/AlarmChanged</uei>
    <event-label>Alarm %parm[alarmid]% Changed</event-label>
    <descr>
    &lt;p&gt;Alarm %parm[alarmid]% Changed&lt;p&gt;
    
    &lt;p&gt;New Alarm Values&lt;p&gt;
    &lt;p&gt;%parm[newalarmvalues]%&lt;p&gt;
    
    &lt;p&gt;Old Alarm Values&lt;p&gt;
    &lt;p&gt;%parm[oldalarmvalues]%&lt;p&gt;
    </descr>
    <logmsg dest="logndisplay">
    &lt;p&gt;Alarm %parm[alarmid]% Changed&lt;p&gt;
    </logmsg>
    <severity>Normal</severity>
    <operinstruct></operinstruct>
  </event>
  
  <event>
    <uei>uei.opennms.org/plugin/AlarmChangeNotificationEvent/AlarmSeverityChanged</uei>
    <event-label>Alarm %parm[alarmid]% Severity Changed to %parm[severity]%</event-label>
    <descr>
    &lt;p&gt;Alarm %parm[alarmid]% Severity Changed to %parm[severity]%&lt;p&gt;
    
    &lt;p&gt;New Alarm Severity %parm[severity]%&lt;p&gt;
    
    &lt;p&gt;Old Alarm Severity %parm[oldseverity]%&lt;p&gt;
    
    &lt;p&gt;New Alarm Values&lt;p&gt;
    &lt;p&gt;%parm[newalarmvalues]%&lt;p&gt;
    
    </descr>
    <logmsg dest="logndisplay">
    &lt;p&gt;Alarm %parm[alarmid]% Severity Changed to %parm[severity]%&lt;p&gt;
    </logmsg>
    <severity>Normal</severity>
    <operinstruct></operinstruct>
  </event>
  
  
  <event>
    <uei>uei.opennms.org/plugin/AlarmChangeNotificationEvent/AlarmAcknowledged</uei>
    <event-label>Alarm %parm[alarmid]% Acknowledged by user %parm[alarmackuser]%</event-label>
    <descr>
    &lt;p&gt;Alarm %parm[alarmid]% Acknowledged &lt;p&gt;
    
    &lt;p&gt;Alarm Acknowledgement time %parm[alarmacktime]%&lt;p&gt;
    
    &lt;p&gt;Acknowledged by user %parm[alarmackuser]%&lt;p&gt;
    
    &lt;p&gt;New Alarm Values&lt;p&gt;
    &lt;p&gt;%parm[newalarmvalues]%&lt;p&gt;

    </descr>
    <logmsg dest="logndisplay">
    &lt;p&gt;Alarm %parm[alarmid]% Acknowledged by user %parm[alarmackuser]%&lt;p&gt;
    </logmsg>
    <severity>Normal</severity>
    <operinstruct></operinstruct>
  </event>
  
  <event>
    <uei>uei.opennms.org/plugin/AlarmChangeNotificationEvent/AlarmUnAcknowledged</uei>
    <event-label>Alarm %parm[alarmid]% UnAcknowledged</event-label>
    <descr>
    &lt;p&gt;Alarm %parm[alarmid]% UnAcknowledged&lt;p&gt;
    
    &lt;p&gt;New Alarm Values&lt;p&gt;
    &lt;p&gt;%parm[newalarmvalues]%&lt;p&gt;

    </descr>
    <logmsg dest="logndisplay">
    &lt;p&gt;Alarm %parm[alarmid]% UnAcknowledged&lt;p&gt;
    </logmsg>
    <severity>Normal</severity>
    <operinstruct></operinstruct>
  </event>

  <event>
    <uei>uei.opennms.org/plugin/AlarmChangeNotificationEvent/AlarmSuppressed</uei>
    <event-label>Alarm %parm[alarmid]% Suppressed by user %parm[suppresseduser]% until %parm[suppresseduntil]%</event-label>
    <descr>

    &lt;p&gt;Alarm Suppressed %parm[alarmid]%&lt;p&gt;
    
    &lt;p&gt;Alarm Suppressed time %parm[suppressedtime]%&lt;p&gt;
    
    &lt;p&gt;Alarm Suppressed until %parm[suppresseduntil]%&lt;p&gt;
    
    &lt;p&gt;Alarm Suppressed by user %parm[alarmackuser]%&lt;p&gt;
    
    &lt;p&gt;New Alarm Values&lt;p&gt;
    &lt;p&gt;%parm[newalarmvalues]%&lt;p&gt;

    </descr>
    <logmsg dest="logndisplay">
    &lt;p&gt;Alarm %parm[alarmid]% Suppressed by user %parm[suppresseduser]% until %parm[suppresseduntil]%&lt;p&gt;
    </logmsg>
    <severity>Normal</severity>
    <operinstruct></operinstruct>
  </event>

</events>